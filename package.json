{
  "name": "esec-chatbot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
"scripts": {
  "start": "node server/index.js",
  "build": "npm run build",
  "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
  "dev:server": "cd server && npm run dev",
  "dev:client": "cd client && npm start"
},
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^5.4.2"
  }
}

